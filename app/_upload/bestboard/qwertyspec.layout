BLOCK ( ROW 1 COLUMN 1
	BUTTON ( TEXT "~" )
SKIP 10
	BUTTON ( TEXT "_" )
	BUTTON ( TEXT "+" )
DL
	BUTTON ( TEXT "{" )
	BUTTON ( TEXT "}" )
	BUTTON ( TEXT "|" )
DL L L
	BUTTON ( TEXT ":" )
	BUTTON ( TEXT "\"" )
DL L L	
