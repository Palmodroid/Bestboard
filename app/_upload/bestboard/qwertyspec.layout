	BUTTON ( TEXT ">" )
	BUTTON ( TEXT "?" )
	)

DEFAULT ( BUTTON ( COLOR 0nAZURE ))
BLOCK ( ROW 2 COLUMN 3
	BUTTON ( TIME ADDTITLE (ITALICS))
