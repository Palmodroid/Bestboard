COAT ( 1000 )

ADDLAYOUT ( ID qwertyspec ) 

DEFAULT ( BUTTON ( COLOR 0nroyalblue ))
DEFAULT ( ADDTITLE ( COLOR 0nYELLOW ))
BLOCK ( ROW 2 COLUMN 4
