	BUTTON ( TEXT ":-)" ADDTITLE ( TEXT "$1F60A" ) )
	BUTTON ( TEXT ":-(" ADDTITLE ( TEXT "$1F620" ) )
	BUTTON ( TEXT ";-)" ADDTITLE ( TEXT "$1F609" ) )
	BUTTON ( TEXT ":'(" ADDTITLE ( TEXT "$1F622" ) )
	BUTTON ( TEXT "<3" ADDTITLE ( TEXT "$1F495" ) )
	)

DEFAULT ( BUTTON ( COLOR 0nspringgreen ))
DEFAULT ( ADDTITLE (  ))
BLOCK ( ROW 3 COLUMN 3
	BUTTON ( TEXT "\0300" )
	BUTTON ( TEXT "\0301" )
	BUTTON ( TEXT "\0302" )
	BUTTON ( TEXT "\030C" )
	BUTTON ( TEXT "\030A" )
	BUTTON ( TEXT "\0308" )
	BUTTON ( TEXT "\030B" )
	BUTTON ( TEXT "\0327" )
	)
	
DEFAULT ( BUTTON ( COLOR 0ncyan ))
DEFAULT ( ADDTITLE ())
BLOCK ( ROW 1 COLUMN 2
	BUTTON (TEXT "!")
	BUTTON (TEXT "@")
	BUTTON (TEXT "#")
	BUTTON (TEXT "\$")
	BUTTON (TEXT "%")
	BUTTON (TEXT "^")
	BUTTON (TEXT "&")
	BUTTON (TEXT "*")
	BUTTON (TEXT "(")
	BUTTON (TEXT ")")
	)
	
